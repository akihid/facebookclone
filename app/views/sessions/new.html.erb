<h1>ログイン画面</h1>
<%= form_with(scope: :session ,url: sessions_path , local: true ,class:"form form_border")do |f|%>
 <div class="form-group">
  <%= f.label :email ,"メールアドレス"%>
  <%= f.email_field :email ,class:"input_field"%>
</div>
 <div class="form-group">
  <%= f.label :password ,"パスワード"%>
  <%= f.password_field :password ,class:"input_field"%>
</div>
  <%= f.submit "ログイン" , class:"btn btn-default btn-xs"%>

<% end %>